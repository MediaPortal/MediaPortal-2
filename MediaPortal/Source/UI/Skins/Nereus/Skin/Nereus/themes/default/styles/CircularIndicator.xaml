    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="{x:Type Control}">
          <Grid Width="150" Height="150">
            <Rectangle x:Name="Segment" RenderTransformOrigin="0.5,3.4" HorizontalAlignment="Center" Height="22" 
                       VerticalAlignment="Top" Width="10" RadiusY="5" RadiusX="5" Fill="Black" StrokeThickness="0" Opacity="0.2">
              <Rectangle.RenderTransform>
                <RotateTransform Angle="0"/>
              </Rectangle.RenderTransform>
            </Rectangle>
            <Rectangle Margin="2" RenderTransformOrigin="0.5,4.05" HorizontalAlignment="Center" Height="18" VerticalAlignment="Top" 
                       Width="6" RadiusY="3" RadiusX="3" Fill="Black" StrokeThickness="0" Opacity="0.2">
              <Rectangle.RenderTransform>
                <RotateTransform Angle="0"/>
              </Rectangle.RenderTransform>
            </Rectangle>
          </Grid>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>

